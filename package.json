{
  "name": "webpack-bootstrap4-laravel_mix-boilerplate",
  "version": "1.0.0",
  "description": "Bootstrap 4 Boilerplate, Bundled with Webpack, Managed by Laravel Mix",
  "main": "webpack.mix.js",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-poll": "npm run watch -- --watch-poll",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run dev -- --production",
    "start": "npm run dev -- --watch"
    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kalichaudhary/webpack-bootstrap4-laravel_mix-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/kalichaudhary/webpack-bootstrap4-laravel_mix-boilerplate/issues"
  },
  "homepage": "https://github.com/kalichaudhary/webpack-bootstrap4-laravel_mix-boilerplate",
  "keywords": [
    "bootstrap 4",
    "boilerplate",
    "font awesome",
    "webpack",
    "laravel mix"
  ],
  "author": "Kali Chaudhary",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.5.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "browser-sync": "^2.26.12",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^7.0.2",
    "laravel-mix": "^5.0.5",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.12"
  }
}
